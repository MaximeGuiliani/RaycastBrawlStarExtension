{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "test",
  "title": "Test",
  "description": "Extension to test Raycast's API",
  "icon": "command-icon.png",
  "author": "Maxime_GUILIANI",
  "license": "MIT",
  "commands": [

    {
      "name":"hello-world",
      "title": "Hello World",
      "icon": "command-icon.png",
      "description": "A simple command that shows a notification",
      "mode": "view"
    },
    {
      "name": "brawlers",
      "title": "All brawlers",
      "description": "List of all brawlers",
      "mode": "view"
    },
    {
      "name": "club",
      "title": "Search for a club",
      "description": "Search for a club",
      "mode": "view",
      "arguments": [
        {
          "name": "id",
          "placeholder": "clubId",
          "required": true,
          "type": "text"
        }
      ]
    },
    {
      "name": "player",
      "title": "Search for a player",
      "description": "Search for a player",
      "mode": "view",
      "arguments": [
        {
          "name": "id",
          "placeholder": "PlayerId",
          "required": true,
          "type": "text"
        }
      ]
    }
  ],
  "preferences": [
    {
      "name": "personalAccessToken",
      "title": "Personal Access Token",
      "placeholder": "Generate at https://developer.brawlstars.com",
      "description": "See the extension's README for instructions about how to create a token",
      "type": "password",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.54.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
